# initialise root logger with level DEBUG
log4j.rootLogger=DEBUG, Bj
#log4j.appender.Bj=org.apache.log4j.FileAppender
log4j.appender.Bj=org.apache.log4j.ConsoleAppender
#log4j.appender.Bj.File=../logs/netslate.log
log4j.appender.Bj.layout=org.apache.log4j.PatternLayout
log4j.appender.Bj.layout.ConversionPattern=%d{HH:mm:ss} [%c] %p %m%n